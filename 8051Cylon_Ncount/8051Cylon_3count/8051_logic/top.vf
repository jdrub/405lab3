                                 .WE(XLXN_16), 
                                 .DOUT(ram_dataout[7:0]));
   BRAM4kx8  XLXI_11 (.Addr(ROMaddr[11:0]), 
                     .clkin(clk10mHz), 
                     .Dout(ROMdata[7:0]));
