module top(clkin, 
           hlt, 
           resetin, 
           P1out);

    input clkin;
    input [7:0] hlt;
    input resetin;
   output [7:0] P1out;
   
   wire clk10mHz;
   wire [6:0] ram_addr;
   wire [7:0] ram_datain;
   wire [7:0] ram_dataout;
   wire [15:0] ROMaddr;
   wire [7:0] ROMdata;
   wire XLXN_9;
   wire XLXN_10;
   wire XLXN_16;
   
   mc8051_core  XLXI_1 (.all_rxd_i(), 
                       .all_t0_i(), 
                       .all_t1_i(), 
                       .clk(clk10mHz), 
                       .datax_i(), 
                       .int0_i(), 
                       .int1_i(), 
                       .p0_i(), 
                       .p1_i(), 
                       .p2_i(hlt[7:0]), 
                       .p3_i(), 
                       .ram_data_i(ram_dataout[7:0]), 
                       .reset(resetin), 
                       .rom_data_i(ROMdata[7:0]), 
